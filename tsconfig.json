{
    "compilerOptions": {
        "target": "es2016",
        "jsx": "react",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "baseUrl": "./resources/assets",
        "paths": {
            "@/*": ["./*"]
        },
        "skipLibCheck": true
    },
    "include": ["./resources/**/*.ts", "./resources/**/*.tsx"],
}
